<h1>New User</h1>

<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br/>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>

  <%= f.label :email %>
  <%= f.text_field :email %><br/>

  <%= f.label :photo %>
  <%= f.file_field :photo %><br/>

  <%= f.submit %>
<% end %>
